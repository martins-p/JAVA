
public class Quick_Maths {

	public static void main(String[] args) {

		//int c = a + b;
		int a = 8;
		int b = 81;
		
		double c = b/(double)a;

		double d = 8.0;
		double e = b/d;
		
		String zz = "zz11";
		String ii = "ii00";
		
	
		System.out.println(zz + ii);
		System.out.println("a/b = " + c);
		System.out.println("b/e  = " + e);
		
		int x = 5;
		int y = 8;
		
		int z = x % y;
		
		System.out.println("mod" + z);
		
		
	}

}
